{
    "name": "omnipay/powertranz",
    "type": "library",
    "description": "Powertranz gateway for the Omnipay payment processing library",
    "keywords": [
        "gateway",
        "merchant",
        "omnipay",
        "pay",
        "payment",
        "powertranz",
        "purchase"
    ],
    "homepage": "https://github.com/omnipay/omnipay-powertranz",
    "license": "MIT",
    "authors": [
        {
            "name": "Omnipay Contributors",
            "homepage": "https://github.com/omnipay/omnipay-powertranz/contributors"
        }
    ],
    "require": {
        "php": "^7.2 || ^8.0",
        "omnipay/common": "^3.0"
    },
    "require-dev": {
        "omnipay/tests": "^3.0 || ^4.0",
        "phpunit/phpunit": "^8.0 || ^9.0",
        "squizlabs/php_codesniffer": "^3.5",
        "http-interop/http-factory-guzzle": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "Omnipay\\Powertranz\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Omnipay\\Powertranz\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR12 src/",
        "fix-style": "phpcbf -p --standard=PSR12 src/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
